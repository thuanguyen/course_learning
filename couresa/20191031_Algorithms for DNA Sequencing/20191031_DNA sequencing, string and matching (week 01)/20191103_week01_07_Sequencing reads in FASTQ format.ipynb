    "# How to convert the Q quanlity value into the charracter\n",
    "def QtoPhred33(Q):\n",
    "    '''Turn Q into Phred+33 ASCII-encoded quanlity'''\n",
    "    return chr(Q+33)\n",
    "\n",
    "def Pred33ToQ(qual):\n",
    "    '''Turn Phred+33 ASCII-encoded quanlity into Q'''\n",
    "    return ord(qual) - 33\n",
    "\n",
    "\n"
   ]
